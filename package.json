{
  "name": "fp-ts-stream",
  "version": "0.1.2",
  "description": "A library providing async and sync streams for fp-ts.",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./Stream": {
      "types": "./dist/esm/Stream/index.d.ts",
      "import": "./dist/esm/Stream/index.js",
      "require": "./dist/cjs/Stream/index.js"
    },
    "./AsyncStream": {
      "types": "./dist/esm/AsyncStream/index.d.ts",
      "import": "./dist/esm/AsyncStream/index.js",
      "require": "./dist/cjs/AsyncStream/index.js"
    },
    "./Stream/*": {
      "types": "./dist/esm/Stream/*.d.ts",
      "import": "./dist/esm/Stream/*.js",
      "require": "./dist/cjs/Stream/*.js"
    },
    "./AsyncStream/*": {
      "types": "./dist/esm/AsyncStream/*.d.ts",
      "import": "./dist/esm/AsyncStream/*.js",
      "require": "./dist/cjs/AsyncStream/*.js"
    }
  },
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepublish": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build": "rm -rf dist/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build-watch": "tsc --watch",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/incetarik/fp-ts-stream.git"
  },
  "keywords": [
    "stream",
    "fp-ts",
    "async",
    "generator"
  ],
  "author": "Tarik Ince <incetarik@hotmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/incetarik/fp-ts-stream/issues"
  },
  "homepage": "https://github.com/incetarik/fp-ts-stream#readme",
  "dependencies": {
    "fp-ts": "2.13.1",
    "tslib": "2.4.0"
  },
  "devDependencies": {
    "typescript": "4.8.4",
    "release-it": "*"
  }
}
